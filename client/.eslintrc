{
    "parser": "babel-eslint",
    "extends": ["airbnb"],
    "rules": {
        "no-unused-expression": false,
        "indent": [
            true,
            "spaces"
        ],
        "quotemark": [
            true,
            "single",
            "jsx-double"
        ],
        "no-var-requires": false,
        "ordered-imports": false,
        "member-ordering": [
            false
        ],
        "object-literal-sort-keys": false,
        "no-shadowed-variable": false,
        "no-console": [
            false
        ],
        "jsx-alignment": [
            false
        ],
        "jsx-no-multiline-js": false,
        "jsx-no-lambda": false,
        "interface-over-type-literal": false,
        "no-empty": false,
        "ban-types": false,
        "variable-name": {
            "options": [
              "allow-pascal-case"
            ]
          }
    }
}